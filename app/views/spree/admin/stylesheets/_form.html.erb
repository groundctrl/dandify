<%= form_for(admin_stylesheet_path) do |f| %>
  <%= #render "spree/shared/error_messages", :target => @style %>
  <%= f.field_container :style_raw do %>
    <%= f.label :style_raw %>
    <%= f.text_area :style_raw, size: '80x40' %>
  <% end %>
  <fieldset class="no-border-top">
    <%= render 'spree/admin/shared/edit_resource_links' %>
  </fieldset>
<% end %>
